#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

   int main(){
   	int a[101];
   	int t;
   	int ev=0,od=0;
   	cin>>t;
   	for(int i=0; i<t; i++){
   		  cin>>a[i];
   		  if(a[i]%2==0){
   		  	  ev++;
   		  	}
   		  	else{
   		  		od++;
   		  		}
   		}
   		 
   		 if(ev>od){
   		 	cout<<"READY FOR BATTLE"<<endl;
   		 	}
   		 	else{
   		 		cout<<"NOT READY"<<endl;
   		 		}

   return 0;

}
