#include<bits/stdc++.h>
using namespace std;

int main(){
      
      int t;
      cin>>t;
      while(t--){
      	int a;
      	int r=0,res=0;
      	cin>>a;
         
         while(a!=0){
         	 res=a%10;
         	 r+=res;
         	 a=a/10;
             }
         	cout<<r<<endl;
         	
         	
      	}
    return 0;
}
