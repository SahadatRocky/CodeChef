#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

int main(){
   
   	  int n,k,c=0;
   	  
   	  cin>>n>>k;
   	
   	 for(int i=0;i<n;i++){
   	// vector<int>v(n);
   	 int v;
   	       cin>>v;
   	       if(v%k ==0){
   	       	       c++;
   	       	}
   	     
   	  	}
   	  	cout<<c<<endl;
   return 0;

}
