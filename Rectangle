#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;

   int main(){
   	  
   	  int t,a,b,c,d;
   	  
   	  cin>>t;
   	  while(t--){
   	  	
   	  	cin>>a>>b>>c>>d;
   	  	int f=0;
   	  	if((a==b&& c==d)||(a==c&&b==d)||(a==d&&b==c)){
   	  		f=1;
   	  		}
   	  		
   	  	if(f==1){
   	  		cout<<"YES"<<endl;
   	  		}else{
   	  			cout<<"NO"<<endl;
   	  			}	
   	  		
   	  		
   	  	}
   return 0;

}
